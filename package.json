    "pinia": "^2.0.34",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "esbuild": "^0.13.4",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "lodash": "^4.17.21",
    "prettier": "2.8.8",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{js,vue}": "prettier --write",
    "*.{ts,js,vue}": "eslint --cache --fix"
